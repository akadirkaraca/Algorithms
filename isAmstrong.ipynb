    "def isAmstrong(num):\n",
    "    \n",
    "    num_str = str(num)\n",
    "    num_list = [int(i)**len(num_str) for i in num_str]\n",
    "    \n",
    "    if num == sum(num_list):\n",
    "        print(\"Yes! This number is amstrong number\")\n",
    "    else:\n",
    "        print(\"Unfortunately this number is not amstrong number\")"
   ]
